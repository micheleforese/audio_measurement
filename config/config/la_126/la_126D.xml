<proc name="la_126D Calibration">

  <steps>
    <serial_number>
      <text>ID Device:</text>
    </serial_number>

    <file>
      <key>master</key>
      <path>master.calibration.dat</path>
    </file>

    <default>
      <sweep>
        <config>
          <nidaq>
            <Fs_max>1000000</Fs_max>
            <voltage_min>-10</voltage_min>
            <voltage_max>10</voltage_max>
            <input_channel>cDAQ9189-1CDBE0AMod5/ai0</input_channel>
          </nidaq>
          <sampling>
            <Fs_multiplier>50</Fs_multiplier>
            <points_per_decade>20</points_per_decade>
            <frequency_min>5</frequency_min>
            <frequency_max>99999</frequency_max>
            <number_of_samples>100</number_of_samples>
            <delay_measurements>0</delay_measurements>
          </sampling>
          <plot>
            <interpolation_rate>10</interpolation_rate>
          </plot>
        </config>
      </sweep>
    </default>

    <!-- GAIN 7 -->
    <text>Gain 7</text>
    <ask>Confirmed?</ask>
    <file>
      <key>set_level_7</key>
      <path>set_level_7.set_level.dat</path>
    </file>

    <file>
      <key>set_level_7_plot</key>
      <path>set_level_7_plot.png</path>
    </file>

    <file>
      <key>gain_7</key>
      <path>gain_7.dat</path>
    </file>

    <set_level>
      <file_set_level key="set_level_7" />
      <file_set_level_plot key="set_level_7_plot" />
      <config>
        <nidaq>
          <Fs_max>1000000</Fs_max>
          <voltage_min>-10</voltage_min>
          <voltage_max>10</voltage_max>
          <input_channel>cDAQ9189-1CDBE0AMod5/ai0</input_channel>
        </nidaq>
        <sampling>
          <Fs_multiplier>50</Fs_multiplier>
          <number_of_samples>100</number_of_samples>
          <delay_measurements>0</delay_measurements>
        </sampling>
      </config>
    </set_level>

    <insertion_gain>
      <file_calibration key="master" />
      <file_set_level key="set_level_7" />
      <file_gain key="gain_7" />
    </insertion_gain>

    <sweep>
      <name_folder>G 7</name_folder>
      <file_set_level key="set_level_7" />
      <file_offset key="set_level_7" />
      <file_insertion_gain key="gain_7" />
      <config>
        <plot>
          <color>#ff0000</color>
          <legend>G 7</legend>
        </plot>
      </config>
    </sweep>

    <!-- GAIN 10 -->
    <text>Gain 10</text>
    <ask>Confirmed?</ask>

    <file>
      <key>set_level_10</key>
      <path>set_level_10.set_level.dat</path>
    </file>

    <file>
      <key>set_level_10_plot</key>
      <path>set_level_10_plot.png</path>
    </file>

    <file>
      <key>gain_10</key>
      <path>gain_10.dat</path>
    </file>

    <set_level>
      <file_set_level key="set_level_10" />
      <file_set_level_plot key="set_level_10_plot" />
      <config>
        <nidaq>
          <Fs_max>1000000</Fs_max>
          <voltage_min>-10</voltage_min>
          <voltage_max>10</voltage_max>
          <input_channel>cDAQ9189-1CDBE0AMod5/ai0</input_channel>
          <delay_measurements>0</delay_measurements>
        </nidaq>
        <sampling>
          <Fs_multiplier>50</Fs_multiplier>
          <number_of_samples>100</number_of_samples>
        </sampling>
      </config>
    </set_level>

    <insertion_gain>
      <file_calibration key="master" />
      <file_set_level key="set_level_10" />
      <file_gain key="gain_10" />
    </insertion_gain>


    <sweep>
      <name_folder>G 10</name_folder>
      <file_set_level key="set_level_10" />
      <file_offset key="set_level_10" />
      <file_insertion_gain key="gain_10" />
      <config>
        <plot>
          <color>#0000ff</color>
          <legend>G 10</legend>
        </plot>
      </config>
    </sweep>

  </steps>
</proc>