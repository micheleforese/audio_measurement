<proc name="R 503">

  <steps>

    <serial_number>
      <text>ID Device:</text>
    </serial_number>

    <file>
      <key>master</key>
      <path>master.calibration.dat</path>
    </file>

    <file>
      <key>set_level_base</key>
      <path>set_level_base.set_level.dat</path>
    </file>

    <file>
      <key>set_level_base_plot</key>
      <path>set_level_base_plot.png</path>
    </file>

    <file>
      <key>gain_base</key>
      <path>gain_base.dat</path>
