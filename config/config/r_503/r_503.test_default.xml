    <default>
      <sweep>
        <file_set_level key="set_level_base" />
        <file_offset key="set_level_base" />
        <file_insertion_gain key="gain_base" />
        <config>
          <nidaq>
            <Fs_max>1000000</Fs_max>
            <voltage_min>-10</voltage_min>
            <voltage_max>10</voltage_max>
            <input_channel>cDAQ9189-1CDBE0AMod5/ai0</input_channel>
          </nidaq>
          <sampling>
            <Fs_multiplier>50</Fs_multiplier>
            <points_per_decade>6</points_per_decade>
            <frequency_min>5</frequency_min>
            <frequency_max>400000</frequency_max>
            <number_of_samples>100</number_of_samples>
            <delay_measurements>0</delay_measurements>
          </sampling>
          <plot>
            <interpolation_rate>10</interpolation_rate>
          </plot>
        </config>
      </sweep>
    </default>

    <!-- BASE -->
    <text>[Hi] - Gain a 0
[HI] - Forchetta CENTRALE
[Mid] - Gain a 0
[Mid] - F a 1kHz
[Mid] - Campana CENTRALE
[Mid] - Hz CENTRALE
[LO] - Gain a 0
[LO] - Forchetta CENTRALE</text>
    <ask>Confirmed?</ask>

    <set_level>
      <dBu>-10</dBu>
      <file_set_level key="set_level_base" />
      <file_set_level_plot key="set_level_base_plot" />
      <config>
        <nidaq>
          <Fs_max>1000000</Fs_max>
          <voltage_min>-10</voltage_min>
          <voltage_max>10</voltage_max>
          <input_channel>cDAQ9189-1CDBE0AMod5/ai0</input_channel>
        </nidaq>
        <sampling>
          <Fs_multiplier>50</Fs_multiplier>
          <number_of_samples>900</number_of_samples>
        </sampling>
      </config>
    </set_level>

    <insertion_gain>
      <file_calibration key="master" />
      <file_set_level key="set_level_base" />
      <file_gain key="gain_base" />
    </insertion_gain>

    <text>HI Measurements</text>


