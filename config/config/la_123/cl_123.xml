<proc name="cl_123">

  <steps>
    <serial_number>
      <text>ID Device:</text>
    </serial_number>

    <!-- GAIN 1 -->
    <text>
      Collegare L'Output al DUT.
      GAIN 1.
    </text>

    <set_level>
      <file_set_level>
        <key>set_level_1</key>
        <name>set_level_1.set_level.dat</name>
      </file_set_level>
      <file_set_level_plot>
        <key>set_level_1_plot</key>
        <name>set_level_1_plot.set_level.png</name>
      </file_set_level_plot>
      <config>
        <nidaq>
          <Fs_max>1000000</Fs_max>
          <voltage_min>-10</voltage_min>
          <voltage_max>10</voltage_max>
          <input_channel>cDAQ9189-1CDBE0AMod5/ai0</input_channel>
        </nidaq>
        <sampling>
          <Fs_multiplier>50</Fs_multiplier>
          <number_of_samples>900</number_of_samples>
        </sampling>
      </config>
    </set_level>

    <insertion_gain>
      <file_calibration>
        <key>master</key>
        <name>master.calibration.dat</name>
      </file_calibration>
      <file_set_level>
        <key>set_level_1</key>
        <name>set_level_1.set_level.dat</name>
      </file_set_level>
      <file_gain>
        <key>gain_1</key>
        <name>gain_1.dat</name>
      </file_gain>
    </insertion_gain>

    <sweep>
      <name_folder>sweep EQ OFF G1</name_folder>
      <file_set_level>
        <key>set_level_1</key>
        <name>set_level_1.set_level.dat</name>
      </file_set_level>
      <file_offset>
        <key>set_level_1</key>
        <name>set_level_1.set_level.dat</name>
      </file_offset>
      <file_insertion_gain>
        <key>gain_1</key>
        <name>gain_1.dat</name>
      </file_insertion_gain>
      <config>
        <nidaq>
          <Fs_max>1000000</Fs_max>
          <voltage_min>-10</voltage_min>
          <voltage_max>10</voltage_max>
          <input_channel>cDAQ9189-1CDBE0AMod5/ai0</input_channel>
        </nidaq>
        <sampling>
          <Fs_multiplier>50</Fs_multiplier>
          <points_per_decade>10</points_per_decade>
          <frequency_min>10</frequency_min>
          <frequency_max>300000</frequency_max>
          <number_of_samples>900</number_of_samples>
        </sampling>
        <plot>
          <interpolation_rate>10</interpolation_rate>
          <color>#ff0000</color>
          <legend>EQ OFF G1</legend>
        </plot>
      </config>
    </sweep>

    <!-- GAIN 3 -->
    <text>
      GAIN 3.
    </text>

    <set_level>
      <file_set_level>
        <key>set_level_3</key>
        <name>set_level_3.set_level.dat</name>
      </file_set_level>
      <file_set_level_plot>
        <key>set_level_3_plot</key>
        <name>set_level_3_plot.set_level.png</name>
      </file_set_level_plot>
      <config>
        <nidaq>
          <Fs_max>1000000</Fs_max>
          <voltage_min>-10</voltage_min>
          <voltage_max>10</voltage_max>
          <input_channel>cDAQ9189-1CDBE0AMod5/ai0</input_channel>
        </nidaq>
        <sampling>
          <Fs_multiplier>50</Fs_multiplier>
          <number_of_samples>900</number_of_samples>
        </sampling>
      </config>
    </set_level>

    <insertion_gain>
      <file_calibration>
        <key>master</key>
        <name>master.calibration.dat</name>
      </file_calibration>
      <file_set_level>
        <key>set_level_3</key>
        <name>set_level_3.set_level.dat</name>
      </file_set_level>
      <file_gain>
        <key>gain_3</key>
        <name>gain_3.dat</name>
      </file_gain>
    </insertion_gain>

    <sweep>
      <name_folder>sweep EQ OFF G3</name_folder>
      <file_set_level>
        <key>set_level_3</key>
        <name>set_level_3.set_level.dat</name>
      </file_set_level>
      <file_offset>
        <key>set_level_3</key>
        <name>set_level_3.set_level.dat</name>
      </file_offset>
      <file_insertion_gain>
        <key>gain_3</key>
        <name>gain_3.dat</name>
      </file_insertion_gain>
      <config>
        <nidaq>
          <Fs_max>1000000</Fs_max>
          <voltage_min>-10</voltage_min>
          <voltage_max>10</voltage_max>
          <input_channel>cDAQ9189-1CDBE0AMod5/ai0</input_channel>
        </nidaq>
        <sampling>
          <Fs_multiplier>50</Fs_multiplier>
          <points_per_decade>10</points_per_decade>
          <frequency_min>10</frequency_min>
          <frequency_max>300000</frequency_max>
          <number_of_samples>900</number_of_samples>
        </sampling>
        <plot>
          <interpolation_rate>10</interpolation_rate>
          <color>#00ff00</color>
          <legend>EQ OFF G3</legend>
        </plot>
      </config>
    </sweep>


    <multiplot name="multiplot">
      <file_plot>multiplot.png</file_plot>
      <folder_sweep>
        <var>sweep EQ OFF G1</var>
        <var>sweep EQ OFF G3</var>
        <!-- <var>sweep EQ OFF G4</var> -->
      </folder_sweep>
    </multiplot>

  </steps>
</proc>