    </sweep>

    <multiplot name="multiplot">
      <file_plot>multiplot.png</file_plot>
      <folder_sweep>
        <var>sweep EQ OFF G0</var>
        <var>sweep EQ OFF G2</var>
        <var>sweep EQ OFF G4</var>
      </folder_sweep>
    </multiplot>

  </steps>
</proc>