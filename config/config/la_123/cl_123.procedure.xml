<proc name="cl_123">

  <steps>
    <serial_number>
      <text>ID Device:</text>
    </serial_number>

    <text>
      Collegare Output al BYPASS.
    </text>

    <set_level>
      <file_set_level>
        <key>master</key>
        <name>master.calibration.dat</name>
      </file_set_level>
      <file_set_level_plot>
        <key>master_plot</key>
        <name>master_plot.calibration.png</name>
      </file_set_level_plot>
      <config>
        <nidaq>
          <Fs_max>1000000</Fs_max>
          <voltage_min>-10</voltage_min>
          <voltage_max>10</voltage_max>
          <input_channel>cDAQ9189-1CDBE0AMod5/ai0</input_channel>
        </nidaq>
        <sampling>
          <Fs_multiplier>50</Fs_multiplier>
          <number_of_samples>900</number_of_samples>
        </sampling>
      </config>
    </set_level>

    <!-- GAIN 0 -->
    <text>
      Collegare L'Output al DUT.
      GAIN 0.
    </text>

    <set_level>
      <file_set_level>
