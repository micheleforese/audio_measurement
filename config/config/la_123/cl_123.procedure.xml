          <color>#ff0000</color>
          <legend>EQ OFF G2</legend>
        </plot>
      </config>
    </sweep>

    <!-- GAIN 4 -->
    <text>
      Collegare L'Output al DUT.
      GAIN 4.
    </text>

    <set_level>
      <file_set_level>
        <key>set_level_4</key>
        <name>set_level_4.set_level.dat</name>
